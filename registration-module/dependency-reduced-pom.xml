<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
  <modelVersion>4.0.0</modelVersion>
  <groupId>com.example</groupId>
  <artifactId>registration-moudle</artifactId>
  <version>1.0-SNAPSHOT</version>
  <build>
    <plugins>
      <plugin>
        <artifactId>maven-jar-plugin</artifactId>
        <version>3.2.1</version>
        <configuration>
          <archive>
            <manifestEntries>
              <Main-Class>com.example.Main</Main-Class>
            </manifestEntries>
          </archive>
        </configuration>
      </plugin>
      <plugin>
        <artifactId>maven-shade-plugin</artifactId>
        <version>3.2.1</version>
        <executions>
          <execution>
            <phase>package</phase>
            <goals>
              <goal>shade</goal>
            </goals>
          </execution>
        </executions>
      </plugin>
    </plugins>
  </build>
  <dependencies>
    <dependency>
      <groupId>com.example.libs</groupId>
      <artifactId>neurotec-biometrics</artifactId>
      <version>1.0</version>
      <scope>system</scope>
      <systemPath>${project.basedir}/src/main/resources/native/neurotec-biometrics.jar</systemPath>
    </dependency>
    <dependency>
      <groupId>com.example.libs</groupId>
      <artifactId>neurotec-biometrics-client</artifactId>
      <version>1.0</version>
      <scope>system</scope>
      <systemPath>${project.basedir}/src/main/resources/native/neurotec-biometrics-client.jar</systemPath>
    </dependency>
    <dependency>
      <groupId>com.example.libs</groupId>
      <artifactId>neurotec-biometrics-gui</artifactId>
      <version>1.0</version>
      <scope>system</scope>
      <systemPath>${project.basedir}/src/main/resources/native/neurotec-biometrics-gui.jar</systemPath>
    </dependency>
    <dependency>
      <groupId>com.example.libs</groupId>
      <artifactId>neurotec-core</artifactId>
      <version>1.0</version>
      <scope>system</scope>
      <systemPath>${project.basedir}/src/main/resources/native/neurotec-core.jar</systemPath>
    </dependency>
    <dependency>
      <groupId>com.example.libs</groupId>
      <artifactId>neurotec-devices</artifactId>
      <version>1.0</version>
      <scope>system</scope>
      <systemPath>${project.basedir}/src/main/resources/native/neurotec-devices.jar</systemPath>
    </dependency>
    <dependency>
      <groupId>com.example.libs</groupId>
      <artifactId>neurotec-gui</artifactId>
      <version>1.0</version>
      <scope>system</scope>
      <systemPath>${project.basedir}/src/main/resources/native/neurotec-gui.jar</systemPath>
    </dependency>
    <dependency>
      <groupId>com.example.libs</groupId>
      <artifactId>neurotec-licensing</artifactId>
      <version>1.0</version>
      <scope>system</scope>
      <systemPath>${project.basedir}/src/main/resources/native/neurotec-licensing.jar</systemPath>
    </dependency>
    <dependency>
      <groupId>com.example.libs</groupId>
      <artifactId>neurotec-media</artifactId>
      <version>1.0</version>
      <scope>system</scope>
      <systemPath>${project.basedir}/src/main/resources/native/neurotec-media.jar</systemPath>
    </dependency>
    <dependency>
      <groupId>com.example.libs</groupId>
      <artifactId>neurotec-media-processing</artifactId>
      <version>1.0</version>
      <scope>system</scope>
      <systemPath>${project.basedir}/src/main/resources/native/neurotec-media-processing.jar</systemPath>
    </dependency>
    <dependency>
      <groupId>com.example.libs</groupId>
      <artifactId>utils</artifactId>
      <version>1.0</version>
      <scope>system</scope>
      <systemPath>${project.basedir}/src/main/resources/native/utils.jar</systemPath>
    </dependency>
    <dependency>
      <groupId>com.example.libs</groupId>
      <artifactId>xpp3</artifactId>
      <version>1.0</version>
      <scope>system</scope>
      <systemPath>${project.basedir}/src/main/resources/native/xpp3.jar</systemPath>
    </dependency>
  </dependencies>
  <properties>
    <maven.compiler.target>17</maven.compiler.target>
    <maven.compiler.source>17</maven.compiler.source>
    <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
  </properties>
</project>
